    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.0",
    "csv-loader": "^3.0.3",
    "express": "^4.17.1",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "json5": "^2.1.3",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
