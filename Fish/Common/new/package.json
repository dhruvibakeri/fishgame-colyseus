{
  "name": "webpack",
  "version": "1.0.0",
  "description": "Setting up webpack.",
  "private": true,
  "scripts": {
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "webpack",
    "build:all": "npm run build:backend && npm run build:frontend",
    "watch:backend": "nodemon --watch backend -e ts --exec \"ts-node backend/index.ts\" -- --watch=backend-only",
    "watch:frontend": "webpack serve",
    "watch:all": "nodemon --watch backend -e ts --exec \"ts-node backend/index.ts\" -- --watch=backend+frontend",
    "test:backend": "ts-mocha -p tsconfig.backend.json backend/**/*.spec.ts",
    "watch:test:backend": "ts-mocha -p tsconfig.backend.json backend/**/*.spec.ts -w --watch-files backend/**/*.spec.ts",
    "test:frontend": "ts-mocha -p tsconfig.frontend.json frontend/**/*.spec.ts",
    "watch:test:frontend": "ts-mocha -p tsconfig.frontend.json frontend/**/*.spec.ts -w --watch-files frontend/**/*.spec.ts",
    "clean:build:dist": "rimraf ./dist ./build",
    "clean": "npm run clean:build:dist && rimraf ./app",
    "copyto:app": "ncp dist/ app/dist/ && ncp build/ ./app/build/",
    "bundle": "npm run clean && mkdirp ./app && npm run build:all && npm run copyto:app && npm run clean:build:dist"
  },
  "repository": {
    "type": "git",
